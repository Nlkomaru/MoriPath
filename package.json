		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cloudflare/next-on-pages": "1",
		"@cloudflare/workers-types": "^4.20250303.0",
		"@pandacss/dev": "^0.53.1",
		"@park-ui/panda-preset": "^0.43.1",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"commitizen": "^4.3.1",
